{
    "0": {
        "name": "乾为天（乾卦）",
        "info": {
            "description": "   自强不息\n困龙得水好运交\n不由喜气上眉梢\n一切谋望皆如意\n向后时运渐渐高",
            "level": "上上卦"
        }
    },
    "1": {
        "name": "坤为地（坤卦）",
        "info": {
            "description": "   厚德载物\n肥羊失群入山岗\n饿虎逢之把口张\n适口充肠心欢喜\n卦若占之大吉昌",
            "level": "上上卦"
        }
    },
    "2": {
        "name": "水雷屯（屯卦）",
        "info": {
            "description": "   起始维艰\n风刮乱丝不见头\n颠三倒四犯忧愁\n慢从款来左顺遂\n急促反惹不自由",
            "level": "下下卦"
        }
    },
    "3": {
        "name": "山水蒙（蒙卦）",
        "info": {
            "description": "   启蒙奋发\n卦中爻象犯小耗\n君子占之运不高\n婚姻合伙有琐碎\n做事必然受苦劳",
            "level": "中下卦"
        }
    },
    "4": {
        "name": "水天需（需卦）",
        "info": {
            "description": "   守正待机\n明珠土埋日久深\n无光无亮到如今\n忽然大风吹土去\n自然显露有重新",
            "level": "中上卦"
        }
    },
    "5": {
        "name": "天水讼（讼卦）",
        "info": {
            "description": "   慎争戒讼\n心中有事事难做\n恰是二人争路走\n雨下俱是要占先\n谁肯让谁走一步",
            "level": "中下卦"
        }
    },
    "6": {
        "name": "地水师（师卦）",
        "info": {
            "description": "   行险而顺\n将帅领旨去出征\n骑着烈马拉硬弓\n百步穿杨去得准\n箭中金钱喜气生",
            "level": "中上卦"
        }
    },
    "7": {
        "name": "水地比（比卦）",
        "info": {
            "description": "   诚信团结\n顺风行船撒起帆\n上天又助一蓬风\n不用费力逍遥去\n任意而行大亨通",
            "level": "上上卦"
        }
    },
    "8": {
        "name": "风天小畜（小畜卦）",
        "info": {
            "description": "   蓄养待进\n苗逢旱天尽焦梢\n水想云浓雨不浇\n农人仰面长吁气\n是从款来莫心高",
            "level": "下下卦"
        }
    },
    "9": {
        "name": "天泽履（履卦）",
        "info": {
            "description": "   脚踏实地\n凤凰落在西岐山\n长鸣几声出圣贤\n天降文王开基业\n富贵荣华八百年",
            "level": "中上卦"
        }
    },
    "10": {
        "name": "地天泰（泰卦）",
        "info": {
            "description": "   应时而变\n学文满腹入场闱\n三元及第得意回\n从今解去愁和闷\n喜庆平地一声雷",
            "level": "中中卦"
        }
    },
    "11": {
        "name": "天地否（否卦）",
        "info": {
            "description": "   不交不通\n虎落陷坑不堪言\n进前容易退后难\n谋望不遂自己便\n疾病口舌事牵连",
            "level": "中中卦"
        }
    },
    "12": {
        "name": "天火同人（同人卦）",
        "info": {
            "description": "   上下和同\n心中有事犯猜疑\n谋望从前不着实\n幸遇明人来指引\n诸般忧闷自消之",
            "level": "中上卦"
        }
    },
    "13": {
        "name": "火天大有（大有卦）",
        "info": {
            "description": "   顺天依时\n砍树摸雀作事牢\n是非口舌自然消\n婚姻合伙不费力\n若问走失未逃脱",
            "level": "上上卦"
        }
    },
    "14": {
        "name": "地山谦（谦卦）",
        "info": {
            "description": "   内高外低\n天赐贫人一封金\n不争不抢两平分\n彼此分得金到手\n一切谋望皆遂心",
            "level": "中中卦"
        }
    },
    "15": {
        "name": "雷地豫（豫卦）",
        "info": {
            "description": "   顺时依势\n太公插下杏黄旗\n收妖为徒归西岐\n自此青龙得了位\n一旦谋望百事宜",
            "level": "中中卦"
        }
    },
    "16": {
        "name": "泽雷随（随卦）",
        "info": {
            "description": "   随时变通\n泥里步踏这几年\n推车靠崖在眼前\n目下就该再使力\n扒上崖去发财源",
            "level": "中中卦"
        }
    },
    "17": {
        "name": "山风蛊（蛊卦）",
        "info": {
            "description": "   振疲起衰\n卦中爻象如推磨\n顺当为福反为祸\n心中有益且迟迟\n凡事尽从忙处错",
            "level": "中中卦"
        }
    },
    "18": {
        "name": "地泽临（临卦）",
        "info": {
            "description": "   教民保民\n君王无道民倒悬\n常想拨云见青天\n幸逢明主施仁政\n重又安居乐自然",
            "level": "中上卦"
        }
    },
    "19": {
        "name": "风地观（观卦）",
        "info": {
            "description": "   观下瞻上\n卦遇蓬花旱逢河\n生意买卖利息多\n婚姻自有人来助\n出门永不受折磨",
            "level": "中上卦"
        }
    },
    "20": {
        "name": "火雷噬嗑（噬嗑卦）",
        "info": {
            "description": "   刚柔相济\n运拙如同身受饥\n幸得送饭又送食\n适口充腹心欢喜\n忧愁从此渐消移",
            "level": "上上卦"
        }
    },
    "21": {
        "name": "山火贲（贲卦）",
        "info": {
            "description": "   饰外扬质\n近来运转瑞气周\n窈窕淑女君子求\n钟鼓乐之大吉庆\n占者逢之喜临头",
            "level": "中上卦"
        }
    },
    "22": {
        "name": "山地剥（剥卦）",
        "info": {
            "description": "   顺势而止\n鹊遇天晚宿林中\n不知林内先有鹰\n虽然同处心生恶\n卦若逢之是非轻",
            "level": "中下卦"
        }
    },
    "23": {
        "name": "地雷复（复卦）",
        "info": {
            "description": "   寓动于顺\n马氏太公不相合\n世人占之忧疑多\n恩人无义反为怨\n是非平地起风波",
            "level": "中中卦"
        }
    },
    "24": {
        "name": "天雷无妄（无妄卦）",
        "info": {
            "description": "   无妄而得\n飞鸟失机落笼中\n纵然奋飞不能腾\n目下只宜守本分\n妄想扒高万不能",
            "level": "下下卦"
        }
    },
    "25": {
        "name": "山天大畜（大畜卦）",
        "info": {
            "description": "   止而不止\n忧愁常锁两眉头\n千头万绪挂心间\n从今以后防开阵\n任意行而不相干",
            "level": "中上卦"
        }
    },
    "26": {
        "name": "山雷颐（颐卦）",
        "info": {
            "description": "   纯正以养\n太公独钓渭水河\n手执丝杆忧愁多\n时来又遇文王访\n自此永不受折磨",
            "level": "上上卦"
        }
    },
    "27": {
        "name": "泽风大过（大过卦）",
        "info": {
            "description": "   非常行动\n夜晚梦里梦金银\n醒来仍不见一文\n目下只宜求本分\n思想络是空劳神",
            "level": "中下卦"
        }
    },
    "28": {
        "name": "坎为水（坎卦）",
        "info": {
            "description": "   行险用险\n一轮明月照水中\n只见影儿不见踪\n愚夫当财下去取\n摸来摸去一场空",
            "level": "下下卦"
        }
    },
    "29": {
        "name": "离为火（离卦）",
        "info": {
            "description": "   附和依托\n官人来占主高升\n庄农人家产业增\n生意买卖利息厚\n匠艺占之大亨通",
            "level": "中上卦"
        }
    },
    "30": {
        "name": "泽山咸（咸卦）",
        "info": {
            "description": "   相互感应\n运去黄金失色\n时来棒槌发芽\n月令极好无差\n且喜心宽意大",
            "level": "中上卦"
        }
    },
    "31": {
        "name": "雷风恒（恒卦）",
        "info": {
            "description": "   恒心有成\n渔翁寻鱼运气好\n鱼来撞网跑不了\n别人使本挣不来\n谁想一到就凑合",
            "level": "中上卦"
        }
    },
    "32": {
        "name": "天山遁（遁卦）",
        "info": {
            "description": "   遁世救世\n浓云蔽日不光明\n劝君且莫出远行\n婚姻求财皆不利\n提防口舌到门庭",
            "level": "下下卦"
        }
    },
    "33": {
        "name": "雷天大壮（大壮卦）",
        "info": {
            "description": "   壮勿妄动\n卦占工师得大木\n眼前该着走上路\n时来运转多顺当\n有事自管放心宽",
            "level": "中上卦"
        }
    },
    "34": {
        "name": "火地晋（晋卦）",
        "info": {
            "description": "   求进发展\n锄地锄去苗里草\n谁想财帛将人找\n一锄锄出银子来\n这个运气也算好",
            "level": "中上卦"
        }
    },
    "35": {
        "name": "地火明夷（明夷卦）",
        "info": {
            "description": "   晦而转明\n时乖运拙走不着\n急忙过河拆了桥\n恩人无义反为怨\n凡事无功枉受劳",
            "level": "中下卦"
        }
    },
    "36": {
        "name": "风火家人（家人卦）",
        "info": {
            "description": "   诚威治业\n一朵鲜花镜中开\n看着极好取不来\n劝君休把镜花恋\n卦若逢之主可怪",
            "level": "下下卦"
        }
    },
    "37": {
        "name": "火泽睽（睽卦）",
        "info": {
            "description": "   异中求同\n此卦占来运气歹\n如同太公作买卖\n贩猪牛快贩羊迟\n猪羊齐贩断了宰",
            "level": "下下卦"
        }
    },
    "38": {
        "name": "水山蹇（蹇卦）",
        "info": {
            "description": "   险阻在前\n大雨倾地雪满天\n路上行人苦又寒\n拖泥带水费尽力\n事不遂心且耐烦",
            "level": "下下卦"
        }
    },
    "39": {
        "name": "雷水解（解卦）",
        "info": {
            "description": "   柔道致治\n目下月令如过关\n千辛万苦受熬煎\n时来恰相有人救\n任意所为不相干",
            "level": "中上卦"
        }
    },
    "40": {
        "name": "山泽损（损卦）",
        "info": {
            "description": "   损益制衡\n时运不至费心多\n比作推车受折磨\n山路崎岖吊下耳\n左插右按按不着",
            "level": "下下卦"
        }
    },
    "41": {
        "name": "风雷益（益卦）",
        "info": {
            "description": "   损上益下\n时来运转吉气发\n多年枯木又开花\n枝叶重生多茂盛\n几人见了几人夸",
            "level": "上上卦"
        }
    },
    "42": {
        "name": "泽天夬 （夬卦）",
        "info": {
            "description": "   决而能和\n蜘蛛脱网赛天军\n粘住游蜂翅翎毛\n幸有大风吹破网\n脱离灾难又逍遥",
            "level": "上上卦"
        }
    },
    "43": {
        "name": "天风姤（姤卦）",
        "info": {
            "description": "   天下有风\n他乡遇友喜气欢\n须知运气福重添\n自今交了顺当运\n向后管保不相干",
            "level": "上卦"
        }
    },
    "44": {
        "name": "泽地萃（萃卦）",
        "info": {
            "description": "   荟萃聚集\n游鱼戏水被网惊\n跳过龙门身化龙\n三尺杨柳垂金线\n万朵桃花显你能",
            "level": "中上卦"
        }
    },
    "45": {
        "name": "地风升（升卦）",
        "info": {
            "description": "   柔顺谦虚\n士人来占必得名\n生意买卖也兴隆\n匠艺逢之交易好\n农间庄稼亦收成",
            "level": "上上卦"
        }
    },
    "46": {
        "name": "泽水困（困卦）",
        "info": {
            "description": "   困境求通\n时运不来好伤怀\n撮上押去把梯抬\n一筒虫翼无到手\n转了上去下不来",
            "level": "中上卦"
        }
    },
    "47": {
        "name": "水风井（井卦）",
        "info": {
            "description": "   求贤若渴\n枯井破费已多年\n一朝流泉出来鲜\n资生济渴人称羡\n时来运转喜自然",
            "level": "上上卦"
        }
    },
    "48": {
        "name": "泽火革（革卦）",
        "info": {
            "description": "   顺天应人\n苗逢旱天渐渐衰\n幸得天恩降雨来\n忧去喜来能变化\n求谋干事遂心怀",
            "level": "上上卦"
        }
    },
    "49": {
        "name": "火风鼎（鼎卦）",
        "info": {
            "description": "   稳重图变\n莺鹜蛤蜊落沙滩\n蛤蜊莺鹜两翅扇\n渔人进前双得利\n失走行人却自在",
            "level": "中下卦"
        }
    },
    "50": {
        "name": "震为雷（震卦）",
        "info": {
            "description": "   临危不乱\n一口金钟在淤泥\n人人拿着当玩石\n忽然一日钟悬起\n响亮一声天下知",
            "level": "中上卦"
        }
    },
    "51": {
        "name": "艮为山（艮卦）",
        "info": {
            "description": "   动静适时\n财帛常打心头走\n可惜眼前难到手\n不如意时且忍耐\n逢着闲事休开口",
            "level": "中下卦"
        }
    },
    "52": {
        "name": "风山渐（渐卦）",
        "info": {
            "description": "   渐进蓄德\n俊鸟幸得出笼中\n脱离灾难显威风\n一朝得意福力至\n东西南北任意行",
            "level": "上上卦"
        }
    },
    "53": {
        "name": "雷泽归妹（归妹卦）",
        "info": {
            "description": "   立家兴业\n求鱼须当向水中\n树上求之不顺情\n受尽爬揭难随意\n劳而无功运平平",
            "level": "下下卦"
        }
    },
    "54": {
        "name": "雷火丰（丰卦）",
        "info": {
            "description": "   日中则斜\n古镜昏暗好几年\n一朝磨明似月圆\n君子谋事逢此卦\n时来运转喜自然",
            "level": "上上卦"
        }
    },
    "55": {
        "name": "火山旅（旅卦）",
        "info": {
            "description": "   依义顺时\n飞鸟树上垒窝巢\n小人使计举火烧\n君占此卦为不吉\n一切谋望枉徒劳",
            "level": "下下卦"
        }
    },
    "56": {
        "name": "巽为风（巽卦）",
        "info": {
            "description": "   谦逊受益\n一叶孤舟落沙滩\n有篙无水进退难\n时逢大雨江湖溢\n不用费力任往返",
            "level": "中上卦"
        }
    },
    "57": {
        "name": "兑为泽（泽卦）",
        "info": {
            "description": "   刚内柔外\n这个卦象真可取\n觉着做事不费力\n休要错过这机关\n事事觉得随心意",
            "level": "上上卦"
        }
    },
    "58": {
        "name": "风水涣（涣卦）",
        "info": {
            "description": "   拯救涣散\n隔河望见一锭金\n欲取岸宽水又深\n指望资财难到手\n昼思夜想枉费心",
            "level": "下下卦"
        }
    },
    "59": {
        "name": "水泽节（节卦）",
        "info": {
            "description": "   万物有节\n时来运转喜气生\n登台封神姜太公\n到此诸神皆退位\n纵然有祸不成凶",
            "level": "上上卦"
        }
    },
    "60": {
        "name": "风泽中孚（中孚卦）",
        "info": {
            "description": "   诚信立身\n路上行人色匆匆\n急忙无桥过薄冰\n小心谨慎过得去\n一步错了落水中",
            "level": "下下卦"
        }
    },
    "61": {
        "name": "雷山小过（小过卦）",
        "info": {
            "description": "   行动有度\n行人路过独木桥\n心内惶恐眼里瞧\n爽利保你过得去\n慢行一定不安牢",
            "level": "中上卦"
        }
    },
    "62": {
        "name": "水火既济（既济卦）",
        "info": {
            "description": "   盛极将衰\n金榜以上题姓名\n不负当年苦用功\n人逢此卦名吉庆\n一切谋望大亨通",
            "level": "中上卦"
        }
    },
    "63": {
        "name": "火水未济（未济卦）",
        "info": {
            "description": "   事业未竟\n离地着人几丈深\n是防偷营劫寨人\n后封太岁为凶煞\n时加谨慎祸不侵",
            "level": "中下卦"
        }
    }
}